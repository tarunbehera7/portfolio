{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}