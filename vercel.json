{
    "version": 2,
    "builds": [
      {
        "src": "dist/*.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/dist/$1",
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/dist/index.html"
      }
    ]
  }